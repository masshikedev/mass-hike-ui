{
  "name": "mass-hike-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nuscout/eslint-config": "^0.2.0",
    "eslint": "^4.16.0",
    "prettier": "^1.10.2",
    "prismic-javascript": "^1.4.1",
    "prismic-reactjs": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "~4.2.2",
    "surge": "^0.19.0",
    "react-scripts": "~1.1.0",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "lint": "eslint *.js",
    "stage":
      "react-scripts build && surge build --domain mass-hike-stage.surge.sh"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": ["prettier --write", "git add"]
  }
}
